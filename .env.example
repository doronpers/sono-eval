# Sono-Eval Configuration

# Application
APP_NAME=sono-eval
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# API Server
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# Database
DATABASE_URL=sqlite:///./sono_eval.db
# For PostgreSQL: postgresql://user:password@localhost:5432/sono_eval

# Redis (for caching and task queues)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# MemU Configuration
MEMU_STORAGE_PATH=./data/memory
MEMU_MAX_DEPTH=5
MEMU_CACHE_SIZE=1000

# T5 Model Configuration
T5_MODEL_NAME=t5-base
T5_CACHE_DIR=./models/cache
T5_LORA_RANK=8
T5_LORA_ALPHA=16
T5_LORA_DROPOUT=0.1

# TagStudio Configuration
TAGSTUDIO_ROOT=./data/tagstudio
TAGSTUDIO_AUTO_TAG=true

# Assessment Configuration
ASSESSMENT_ENGINE_VERSION=1.0
ASSESSMENT_ENABLE_EXPLANATIONS=true
ASSESSMENT_MULTI_PATH_TRACKING=true
DARK_HORSE_MODE=enabled

# Superset Configuration
SUPERSET_HOST=localhost
SUPERSET_PORT=8088
SUPERSET_SECRET_KEY=change_this_secret_key_in_production

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ALLOWED_HOSTS=localhost,127.0.0.1

# File Upload
MAX_UPLOAD_SIZE=10485760
ALLOWED_EXTENSIONS=py,js,ts,java,cpp,c,go,rs,rb

# Batch Processing
BATCH_SIZE=32
MAX_CONCURRENT_ASSESSMENTS=4
